{
  "manifest_version": 2,
  "name": "Storyblok Tools",
  "version": "1.0",
  "description": "Adds a red border to all webpages matching mozilla.org.",
  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["index.js"]
    }
  ]
}
